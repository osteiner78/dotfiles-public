                [:!TOj [:yabai "-m window --swap south"]]
                [:!TOk [:yabai "-m window --swap north"]]
                [:!TOh [:yabai "-m window --swap west"]]
                [:!TOl [:yabai "-m window --swap east"]]

;;              ============== YABAI - DISPLAY NAVIGATION =================
