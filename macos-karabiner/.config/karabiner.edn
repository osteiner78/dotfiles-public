                ;; [:v [["/opt/homebrew/bin/yabai w--restart-service"] [:notification "Retarted" "Yabai"]]]
]}

        {:des "Yabai rules - Outside yabai layer (tab)"
        :rules[
;;              ============== YABAI - WINDOWS NAVIGATION =================
